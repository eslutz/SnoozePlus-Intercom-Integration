  "keywords": [
    "intercom",
    "inbox",
    "integration",
    "snooze",
    "snooze+",
    "snooze-plus"
  ],
  "main": "app.js",
  "scripts": {
    "start:dev": "node src/app.js",
    "copy-app": "cp -r src/ dist/SnoozePlus/ && cp package.json dist/SnoozePlus/package.json",
